<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:DirtyPGP" x:Class="DirtyPGP.MainWindow"
        Title="DirtyPGP by mikescher.de" Height="390" Width="625">
    <Grid>
        <TabControl TabStripPlacement="Bottom">
            <TabItem Header="Generate">
                <local:Tab_Generate x:Name="tab_Generate"/>
            </TabItem>
            <TabItem Header="RSA Encrypt">
                <local:Tab_RSA_Encrypt  x:Name="tab_RSA_Encrypt"/>
            </TabItem>
            <TabItem Header="RSA Decrypt">
                <local:Tab_RSA_Decrypt  x:Name="tab_RSA_Decrypt"/>
            </TabItem>
            <TabItem Header="PGP Encrypt">
                <local:Tab_PGP_Encrypt  x:Name="tab_PGP_Encrypt"/>
            </TabItem>
            <TabItem Header="PGP Decrypt">
                <local:Tab_PGP_Decrypt  x:Name="tab_PGP_Decrypt"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
